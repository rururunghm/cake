<div class="container">
  <p id ="notice"></p>
  
  <div class="col-sm-2 text-dark bg-light text-center mb-4 ml-5"><h4>配送先登録/一覧</h4></div>
  
    <%= form_with model: @derivries do |f| %>
      <h4>郵便番号</h4>
        <%= f.file_field :gip_code %>
        
      <h4>住所</h4>
        <%= f.text_field :address %>
        
      <h4>宛名</h4>
        <%= f.text_area :address_name %>
        <%= f.submit "新規登録", class: "btn btn-success" %>
    <% end %>
  
  
</div>